{
  "name": "efh-autopilot",
  "type": "module",
  "version": "2.0.0",
  "engines": {
    "node": "20.11.1",
    "npm": ">=10.0.0"
  },
  "packageManager": "pnpm@9.7.0",
  "dependencies": {
    "@supabase/supabase-js": "2.57.4",
    "@vitejs/plugin-react": "4.7.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-slow-down": "^3.0.0",
    "express-useragent": "^1.0.15",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.1",
    "joi": "^18.0.1",
    "jsdom": "23.2.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-fetch": "3.3.2",
    "pg": "^8.16.3",
    "prop-types": "15.8.1",
    "quill": "^2.0.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-helmet-async": "2.0.5",
    "react-router-dom": "7.9.3",
    "rollup-plugin-visualizer": "6.0.3",
    "slugify": "1.6.6",
    "stripe": "^19.1.0",
    "vite": "6.3.6",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "xss-clean": "^0.1.4",
    "y-websocket": "^3.0.0",
    "yjs": "^13.6.27"
  },
  "devDependencies": {
    "@prisma/client": "^6.16.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.3",
    "@types/express-validator": "^3.0.2",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.7.0",
    "@types/pg": "^8.15.5",
    "@types/winston": "^2.4.4",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^19.0.2",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "rimraf": "5.0.10",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "autopilot:server": "node server.mjs",
    "autopilot:test": "curl -s http://localhost:3001/api/autopilot/status | jq .",
    "dev": "vite --host 0.0.0.0 --port 8012 --strictPort",
    "build": "vite build && npm run build:manifest",
    "build:manifest": "node scripts/build-pages-manifest.mjs",
    "start": "vite preview --host 0.0.0.0 --port 8080 --strictPort",
    "preview": "vite preview --host 0.0.0.0 --port 8080 --strictPort",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "verify": "pnpm typecheck && pnpm lint && pnpm build",
    "routes:check": "node scripts/routes-cli.cjs check",
    "routes:fix": "node scripts/routes-cli.cjs fix",
    "routes:list": "node scripts/routes-cli.cjs list",
    "routes:scan": "node scripts/routes-cli.cjs scan",
    "routes:guardian": "bash scripts/routing-guardian.sh",
    "routes:guardian:dry": "DRY_RUN=1 bash scripts/routing-guardian.sh",
    "serve:keepalive": "bash scripts/dev-ensure.sh",
    "health:once": "node scripts/healthcheck.mjs --base http://localhost:8080",
    "health:loop": "bash -c 'while true; do node scripts/healthcheck.mjs --base http://localhost:8080 || true; sleep 10; done'",
    "autopilot": "bash scripts/autopilot-loop.sh",
    "autopilot:dry": "DRY_RUN=1 bash scripts/autopilot-loop.sh",
    "clean:fast": "rimraf .turbo .next/cache dist build coverage .cache",
    "clean:all": "pnpm clean:fast && rimraf node_modules && pnpm store prune",
    "clean:autopilot": "node scripts/autopilot-cleanup.js",
    "health": "bash efh-healthcheck.sh",
    "validate": "node validate-wix-content.cjs",
    "dns": "node autopilot-wix-pointing.cjs",
    "dns:watch": "node autopilot-wix-pointing.cjs --watch=300",
    "blog:publish": "node wix-blog-system/push-blog.mjs",
    "blog:test": "node wix-blog-system/autopilot-blog-integration.js test",
    "blog:success": "node wix-blog-system/autopilot-blog-integration.js successStory",
    "blog:program": "node wix-blog-system/autopilot-blog-integration.js programUpdate"
  }
}
